within {{project_name}}.Districts;
model TimeSeriesMassFlow_Temp_CentralPlant
  "Heating and Cooling Water massflowrates and temperatures from building on EP."

extends Modelica.Icons.Example;
  {% raw %}package MediumW=Buildings.Media.Water;
  {% endraw %}parameter String filNam="modelica://{{project_name}}/Loads/{{data['load_resources_path']}}/{{data['time_series']['filename']}}";
  {% raw %}parameter Modelica.SIunits.MassFlowRate mChW_flow_nominal=getPeakMassFlowRate(
    string="#Nominal chilled water mass flow rate",
    filNam=Modelica.Utilities.Files.loadResource(filNam))
    "Nominal cooling water flow rate"
    annotation (Dialog(group="Design parameter"));
   parameter Integer nBui= 1
    "Number of buildings";
   parameter Modelica.SIunits.Pressure dpSetPoi=70000
    "Differential pressure setpoint";
   parameter Modelica.SIunits.PressureDifference dp_nominal= dpSetPoi + nBui * 7000
    "District network pressure drop";
 //Building thermal loads ( mass flow rates and water supply and return temperature) from the CSV file
   parameter Modelica.SIunits.MassFlowRate mHW_flow_nominal=
    Buildings.Experimental.DistrictHeatingCooling.SubStations.VaporCompression.BaseClasses.getPeakMassFlowRate(
    string="#Nominal heating water mass flow rate",
    filNam=Modelica.Utilities.Files.loadResource(filNam))
    "Nominal heating water flow rate"
    annotation (Dialog(group="Design parameter"));
   parameter Modelica.SIunits.TemperatureDifference delTBuiChW(displayUnit="degC")=7
    "Nominal chilled water temperature differnce(building side)";
   parameter Modelica.SIunits.TemperatureDifference delTBuiHW(displayUnit="degC")=15
    "Nominal heating water temperature difference(building side) ";
   parameter Modelica.SIunits.TemperatureDifference delTDisChW(displayUnit="degC")=9
    "Nominal chilled water temperature differnce(district side)";
   parameter Modelica.SIunits.TemperatureDifference delTDisHW(displayUnit="degC")=20
    "Nominal heating water temperature difference(district side) ";

   inner parameter
      Buildings.Applications.DHC.Examples.Combined.Generation5.Unidirectional.Data.DesignDataParallel4GDC
      datDesHea(
      nBui=nBui,
      mDis_flow_nominal=heaETS.mDis_flow_nominal,
      mCon_flow_nominal={heaETS.mDis_flow_nominal},
      lDis=fill(5, nBui),
      lCon=fill(5, nBui),
      lEnd=5,
      dhDis={0.05},
      dhCon=fill(0.05, nBui)) "Design data of heating distrcit system."
      annotation (Placement(transformation(extent={{-100,-80},{-80,-60}})));
   Modelica.Blocks.Sources.CombiTimeTable buiMasTem(
      tableOnFile=true,
      tableName="modelica",
      fileName=Modelica.Utilities.Files.loadResource(filNam),
      extrapolation=Modelica.Blocks.Types.Extrapolation.Periodic,
      columns=2:7,
      smoothness=Modelica.Blocks.Types.Smoothness.LinearSegments)
      annotation (Placement(transformation(extent={{160,0},{140,20}})));
   Buildings.Fluid.Sources.MassFlowSource_T supChiWat(
      redeclare package Medium = MediumW,
      use_m_flow_in=true,
      use_T_in=true,
      nPorts=1) "Chilled water supply" annotation (Placement(transformation(
        extent={{10,-10},{-10,10}},
        rotation=0,
        origin={72,40})));
    EnergyTransferStations.CoolingIndirect cooETS(
      allowFlowReversal1=false,
      allowFlowReversal2=false,
      redeclare package Medium = MediumW,
      mBui_flow_nominal=mChW_flow_nominal,
      mDis_flow_nominal=mChW_flow_nominal*delTBuiChW/delTDisChW,
      dpValve_nominal=6000,
      dp1_nominal=500,
      dp2_nominal=500,
      use_Q_flow_nominal=true,
      Q_flow_nominal=mChW_flow_nominal*4187*delTBuiChW,
      T_a1_nominal(displayUnit="degC") = 278.15,
      T_a2_nominal(displayUnit="degC") = 289.15,
      eta=0.8)
      annotation (Placement(transformation(extent={{20,40},{40,60}})));
    Buildings.Fluid.Sources.Boundary_pT buiCooSin(
      redeclare package Medium = MediumW,
      use_T_in=false,
      nPorts=1) "Building cooling sink." annotation (Placement(transformation(
          extent={{-10,-10},{10,10}},
          rotation=0,
          origin={-10,30})));
    EnergyTransferStations.Heating.HeatingIndirect heaETS(
      allowFlowReversal1=false,
      allowFlowReversal2=false,
      redeclare package Medium = MediumW,
      mBui_flow_nominal=mHW_flow_nominal,
      mDis_flow_nominal=mHW_flow_nominal*delTBuiHW/delTDisHW,
      dpValve_nominal=6000,
      dp1_nominal=500,
      dp2_nominal=500,
      use_Q_flow_nominal=true,
      Q_flow_nominal=mHW_flow_nominal*4187*delTBuiHW,
      T_a1_nominal=338.15,
      T_a2_nominal=315.15,
      eta=0.8,
      xi_start=0,
      reverseActing=true)
      annotation (Placement(transformation(extent={{20,-40},{40,-20}})));
    Buildings.Fluid.Sources.MassFlowSource_T supHeaWat(
      redeclare package Medium = MediumW,
      use_m_flow_in=true,
      use_T_in=true,
      nPorts=1) "Heating water supply" annotation (Placement(transformation(
          extent={{10,-10},{-10,10}},
          rotation=0,
          origin={72,-60})));
    Buildings.Fluid.Sources.Boundary_pT buiHeaSin(
      redeclare package Medium = MediumW,
      use_T_in=false,
      nPorts=1) "Building heating sink." annotation (Placement(transformation(
          extent={{-10,-10},{10,10}},
          rotation=0,
          origin={-10,-50})));
    Buildings.Controls.OBC.UnitConversions.From_degC TChWR
      annotation (Placement(transformation(extent={{120,40},{100,60}})));
    Buildings.Controls.OBC.UnitConversions.From_degC THWR
      "Heating water temperature."
      annotation (Placement(transformation(extent={{120,-60},{100,-40}})));
    Buildings.Controls.OBC.UnitConversions.From_degC TChWSET
      annotation (Placement(transformation(extent={{40,100},{20,120}})));
    Buildings.Controls.OBC.UnitConversions.From_degC THWSET
      annotation (Placement(transformation(extent={{40,-90},{20,-70}})));
    Modelica.Blocks.Sources.RealExpression TSetHeaDis(y=55 + 273.15)
      "Heating water setpoint temperature (district level)."
      annotation (Placement(transformation(extent={{-80,-50},{-100,-30}})));
    Modelica.Blocks.Sources.RealExpression mBuiHW(y=buiMasTem.y[5])
      annotation (Placement(transformation(extent={{126,-34},{106,-14}})));
    Modelica.Blocks.Sources.RealExpression mBuiChW(y=buiMasTem.y[6])
      annotation (Placement(transformation(extent={{120,70},{100,90}})));
    CentralPlants.Heating.Generation4.CentralHeatingPlant cenHeaPla(
      mHW_flow_nominal=mHW_flow_nominal,
      dpHW_nominal=dpHW_nominal,
      QBoi_flow_nominal=QBoi_nominal,
      mMin_flow=mMinHea_flow,
      mBoi_flow_nominal=mBoi_flow_nominal,
      dpBoi_nominal=10000,
      delT_nominal(displayUnit="degC") = 10,
      perHWPum=perHWPum,
      tWai=tWai,
      dpSetPoi=dpSetPoi,
      energyDynamics=Modelica.Fluid.Types.Dynamics.FixedInitial)
      "Central heating plant."
      annotation (Placement(transformation(extent={{-100,-20},{-80,0}})));
    Validation.BaseClasses.Distribution2Pipe disNetHea(
      redeclare final package Medium = MediumW,
      final nCon=nBui,
      iConDpSen=1,
      final mDis_flow_nominal=datDesHea.mDis_flow_nominal,
      final mCon_flow_nominal=datDesHea.mCon_flow_nominal,
      final allowFlowReversal=allowFlowReversal,
      dpDis_nominal=dpDisHea_nominal) "Distribution network."
      annotation (Placement(transformation(extent={{-60,-26},{-36,-16}})));
    CentralPlants.Cooling.Plant cenCooPla(
      redeclare
        Buildings.Fluid.Chillers.Data.ElectricEIR.ElectricEIRChiller_Trane_CGWD_207kW_3_99COP_None
        perChi,
      dT_nominal=5.56,
      perCHWPum=perCHWPum,
      perCWPum=perCWPum,
      mCHW_flow_nominal=mChW_flow_nominal,
      dpCHW_nominal=dpCHW_nominal,
      QEva_nominal=QEva_nominal,
      mMin_flow=mMinCoo_flow,
      mCW_flow_nominal=mCW_flow_nominal,
      dpCW_nominal=dpCW_nominal,
      TAirInWB_nominal=298.7,
      TCW_nominal=308.15,
      TMin=288.15,
      PFan_nominal=5000,
      dpCHWPum_nominal=dpCHWPum_nominal,
      dpCWPum_nominal=dpCWPum_nominal,
      tWai=tWai,
      dpSetPoi=dpSetPoi,
      energyDynamics=Modelica.Fluid.Types.Dynamics.FixedInitial)
      "Central cooling plant."
      annotation (Placement(transformation(extent={{-100,60},{-80,80}})));
    Validation.BaseClasses.Distribution2Pipe disNetCoo(
      redeclare final package Medium = MediumW,
      final nCon=nBui,
      iConDpSen=1,
      final mDis_flow_nominal=datDesCoo.mDis_flow_nominal,
      final mCon_flow_nominal=datDesCoo.mCon_flow_nominal,
      final allowFlowReversal=allowFlowReversal,
      dpDis_nominal=dpDisCoo_nominal) "Distribution network."
      annotation (Placement(transformation(extent={{-60,54},{-36,64}})));
    Modelica.Blocks.Sources.BooleanConstant onCoo
      "On signal of the cooling plant."
      annotation (Placement(transformation(extent={{-140,100},{-120,120}})));
    Modelica.Blocks.Sources.BooleanConstant onHea
      "On signal of the heating plant."
      annotation (Placement(transformation(extent={{-140,0},{-120,20}})));
    BoundaryConditions.WeatherData.ReaderTMY3 weaDat(final
        computeWetBulbTemperature=true, filNam=
          Modelica.Utilities.Files.loadResource("modelica://Buildings/Resources/weatherdata/USA_CA_San.Francisco.Intl.AP.724940_TMY3.mos"))
      annotation (Placement(transformation(extent={{-160,40},{-140,60}})));
    BoundaryConditions.WeatherData.Bus weaBus "Weather data bus"
      annotation (Placement(transformation(extent={{-174,66},{-154,86}}),
          iconTransformation(extent={{-114,-54},{-94,-34}})));
    Modelica.Blocks.Sources.RealExpression TSetCooDis(y=5 + 273.15)
      "Chilled water setpoint temperature (district level)."
      annotation (Placement(transformation(extent={{-140,72},{-120,92}})));
equation
    connect(cooETS.port_a2, supChiWat.ports[1]) annotation (Line(points={{40,44},{
            52,44},{52,40},{62,40}}, color={0,127,255}));
    connect(buiCooSin.ports[1], cooETS.port_b2) annotation (Line(points={{0,30},{10,
            30},{10,44},{20,44}}, color={0,127,255}));
    connect(THWSET.y, heaETS.TSetBuiSup) annotation (Line(points={{18,-80},{10,-80},
            {10,-30},{18,-30}}, color={0,0,127}));
    connect(TChWSET.y, cooETS.TSetBuiSup) annotation (Line(points={{18,110},{10,110},
            {10,50},{18,50}}, color={0,0,127}));
    connect(TChWR.y, supChiWat.T_in) annotation (Line(points={{98,50},{90,50},{90,
            44},{84,44}}, color={0,0,127}));
    connect(THWR.y, supHeaWat.T_in) annotation (Line(points={{98,-50},{94,-50},{94,
            -56},{84,-56}},color={0,0,127}));
    connect(buiHeaSin.ports[1], heaETS.port_b2) annotation (Line(points={{0,-50},{
            10,-50},{10,-36},{20,-36}},color={0,127,255}));
    connect(supHeaWat.ports[1], heaETS.port_a2) annotation (Line(points={{62,-60},
            {50,-60},{50,-36},{40,-36}},color={0,127,255}));
    connect(mBuiHW.y, supHeaWat.m_flow_in) annotation (Line(points={{105,-24},{96,
            -24},{96,-52},{84,-52}},color={0,0,127}));
    connect(buiMasTem.y[1], THWR.u) annotation (Line(points={{139,10},{130,10},{130,
            -50},{122,-50}},color={0,0,127}));
    connect(buiMasTem.y[2], THWSET.u) annotation (Line(points={{139,10},{130,10},{
            130,-80},{42,-80}},color={0,0,127}));
    connect(buiMasTem.y[3], TChWR.u) annotation (Line(points={{139,10},{130,10},{130,
            50},{122,50}},color={0,0,127}));
    connect(buiMasTem.y[4], TChWSET.u) annotation (Line(points={{139,10},{130,10},
            {130,110},{42,110}},color={0,0,127}));
    connect(mBuiChW.y, supChiWat.m_flow_in) annotation (Line(points={{99,80},{88,80},
            {88,48},{84,48}}, color={0,0,127}));
    connect(cenHeaPla.port_a, disNetHea.port_bDisRet) annotation (Line(points={{-80,
            -5},{-64,-5},{-64,-24},{-60,-24}}, color={0,127,255}));
    connect(cenHeaPla.port_b, disNetHea.port_aDisSup) annotation (Line(points={{-80,
            -15},{-64,-15},{-64,-21},{-60,-21}}, color={0,127,255}));
    connect(disNetHea.ports_bCon[1], heaETS.port_a1) annotation (Line(points={{-55.2,
            -16},{-56,-16},{-56,0},{0,0},{0,-24},{20,-24}}, color={0,127,255}));
    connect(heaETS.port_b1, disNetHea.ports_aCon[1]) annotation (Line(points={{40,-24},
            {48,-24},{48,0},{-40.8,0},{-40.8,-16}},      color={0,127,255}));
    connect(cenCooPla.port_b, disNetCoo.port_aDisSup) annotation (Line(points={{-80,
            65},{-72,65},{-72,59},{-60,59}}, color={0,127,255}));
    connect(cenCooPla.port_a, disNetCoo.port_bDisRet) annotation (Line(points={{-80,
            75},{-72,75},{-72,56},{-60,56}}, color={0,127,255}));
    connect(disNetCoo.ports_bCon[1], cooETS.port_a1) annotation (Line(points={{-55.2,
            64},{-56,64},{-56,80},{0,80},{0,56},{20,56}}, color={0,127,255}));
    connect(cenCooPla.on, onCoo.y) annotation (Line(points={{-102,78},{-110,78},{-110,
            110},{-119,110}}, color={255,0,255}));
    connect(cenHeaPla.on, onHea.y) annotation (Line(points={{-102,-2},{-110,-2},{-110,
            10},{-119,10}}, color={255,0,255}));
    connect(disNetCoo.ports_aCon[1], cooETS.port_b1) annotation (Line(points={{-40.8,
            64},{-40.8,80},{52,80},{52,56},{40,56}}, color={0,127,255}));
    connect(disNetCoo.dp, cenCooPla.dpMea) annotation (Line(points={{-35.4,60.5},{
            -28,60.5},{-28,40},{-114,40},{-114,67},{-102,67}}, color={0,0,127}));
    connect(disNetHea.dp, cenHeaPla.dpMea) annotation (Line(points={{-35.4,-19.5},
            {-30,-19.5},{-30,-52},{-136,-52},{-136,-13},{-102,-13}}, color={0,0,127}));
    connect(cenHeaPla.THeaSet, TSetHeaDis.y) annotation (Line(points={{-102,-18.4},
            {-120,-18.4},{-120,-40},{-101,-40}}, color={0,0,127}));
    connect(weaDat.weaBus,weaBus)  annotation (Line(
        points={{-140,50},{-134,50},{-134,76},{-164,76}},
        color={255,204,51},
        thickness=0.5));
    connect(weaBus.TWetBul, cenCooPla.TWetBul) annotation (Line(
        points={{-164,76},{-134,76},{-134,62},{-102,62}},
        color={255,204,51},
        thickness=0.5));
    connect(TSetCooDis.y, cenCooPla.TCHWSupSet) annotation (Line(points={{-119,82},
            {-114,82},{-114,73},{-102,73}}, color={0,0,127}));

annotation (Diagram(coordinateSystem(extent={{-160,-100},{160,140}})),
      experiment(StopTime=3600,
      Tolerance=0.0001,
      __Dymola_Algorithm="Dassl"),
      Documentation(
        info="<html>
  <p>
    This example connects the building side (time series CSV file generated from
    energyPlus of heating/cooling water mass flow rate and temperature's)
    with the district side (central plants of cooling and heating) through
    an indirect heating and cooling energy transfer station ETS.
  </p>
  </html>", revisions="<html>
  <ul>
  <li>
  August 25, 2020, by Hagar Elarga:<br/>
  First implementation.
  </li>
  </ul>
  </html>"));
end TimeSeriesMassFlow_Temp_CentralPlant;
